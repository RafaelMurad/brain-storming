{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "react-native",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@components/*": ["components/*"],
      "@constants/*": ["constants/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "extends": "expo/tsconfig.base"
}
